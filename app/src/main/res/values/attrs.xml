<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="typeface">
        <enum name="roboto_regular" value="0"/>
        <enum name="roboto_medium" value="1"/>
        <enum name="roboto_light" value="2"/>
    </attr>

    <declare-styleable name="textView">
        <attr name="typeface"/>
    </declare-styleable>

    <attr name="themeName" format="string">
        <enum name="themeDefault" value="1"/>
        <enum name="themeDark" value="2"/>
    </attr>

</resources>